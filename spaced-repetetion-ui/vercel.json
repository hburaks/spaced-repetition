{
  "version": 2,
  "builds": [
    {
      "src": "dist/spaced-repetition-ui",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@next_public_api_url",
    "HMAC_SECRET": "@hmac_secret"
  }
}
